{
  "name": "payroo",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "dev:server": "bun --watch server/server.ts",
    "dev:all": "bunx concurrently --kill-others \"bun run dev:server\" \"bun run dev\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "bun run server/test/emp.test.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@tanstack/react-query": "^5.62.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "lucide-react": "^0.469.0",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.17.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.1.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bun": "latest",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^7.1.7"
  }
}